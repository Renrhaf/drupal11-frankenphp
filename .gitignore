# Ignore directories generated by Composer
/web/core
/web/autoload.php
/web/index.php
/web/update.php

# Ignore Drupal contrib modules, profiles, themes and libraries
/web/libraries/*
/web/modules/contrib/*
/web/profiles/contrib/*
/web/themes/contrib/*
/web/themes/custom/*/dist/*.css
/web/themes/custom/*/dist/**/*.css

# Ignore Drupal's file directory
/web/sites/default/files/*
!/web/sites/default/files/.htaccess
/data/files/private/*
!/data/files/private/.htaccess
/data/*/files
/data/mysql
/data/redis
/data/elasticsearch
!/data/*/.gitkeep

# Ignore local settings file
/web/sites/settings.local.php
/web/sites/default/services.yml

# Ignore vendors
/vendor

# Ignore bin
/bin/*

# Ignore paths that may contain temporary files
/web/sites/*/translations
/web/sites/*/tmp
/web/sites/*/cache

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Application specific files
node_modules/
.sass-cache/
.idea/
.composer
.unison*

# System files
._*
.DS_Store
Desktop.ini
Thumbs.db
php.ini
/web/sites/sites.php

# Supervisor
supervisord.pid

# Docker compose files
/docker-compose.override.yml

# Docker data
docker/db/data
docker/redis/data

# Env files
.env.local
.env

